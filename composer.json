{
    "name": "erivello/github-api-bundle",
    "description": "Bundle around PHP GitHub API client from KnpLabs",
    "type": "symfony-bundle",
    "keywords": ["api", "github", "gist"],
    "homepage": "https://github.com/erivello/GithubApiBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Edoardo Rivello",
            "email": "edoardo.rivello@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.1",
        "symfony/yaml": ">=2.1",
        "symfony/finder": ">=2.1",
        "knplabs/github-api": "*"
    },
    "minimum-stability": "dev",
    "target-dir": "Erivello/GithubApiBundle",
    "autoload": {
        "psr-0": { "Erivello\\GithubApiBundle": "" }
    }
}
